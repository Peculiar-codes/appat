{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app",
    "dev": "nodemon app.ts",
    "build": "tsc -p .",
    "css": "npx tailwind -i ./public/stylesheets/style.css -o ./public/stylesheets/output --watch",
    "build-image": "docker build . -t Peculiar-codes/appat",
    "run:image": "docker run -p 49160:5000 -d Peculiar-codes/appat",
    "test:docker": "docker ps",
    "call:docker": "curl -i localhost:49160"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "eruda": "^2.5.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^6.5.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "socket.io": "^4.5.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.13",
    "sass": "^1.54.5",
    "typescript": "^4.8.2"
  }
}
